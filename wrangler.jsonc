  "name": "kai-blog",
  "main": "src/index.tsx",
  "compatibility_date": "2025-12-08",
  "pages_build_output_dir": "./dist",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "assets": {
    "directory": "./public"
  },
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "kai-blog-db",
      "database_id": "placeholder-will-be-replaced"
    }
  ]
}
